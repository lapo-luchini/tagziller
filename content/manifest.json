{
  "manifest_version": 2,
  "name": "Tagziller",
  "description": "TagZilla reborn!",
  "version": "0.2.0",
  "author": "Lapo Luchini",
  "applications": {
    "gecko": {
      "id": "tagzilla@lapo.it",
      "strict_min_version": "76.0"
      // "strict_max_version": "78.*"
    }
  },
  "icons": {
    "64": "images/icon-64px.png",
    "32": "images/icon-32px.png",
    "16": "images/icon-16px.png"
  },
  "background": {
    "page": "background.html"
  },
  "compose_action": {
    "default_title": "TagZ"
  },
  "permissions": [
    "accountsRead",
    "compose",
    "storage"
  ]
}
